<transport name="MqttInboundTransport"
	label="${com.esri.geoevent.transport.awsiotmqtt-transport.TRANSPORT_IN_LBL}"
	contact="yourname@yourcompany.com" version="10.4.0"
	domain="com.esri.geoevent.transport.awsiotmqtt.inbound" type="inbound">
	<description>${com.esri.geoevent.transport.awsiotmqtt-transport.TRANSPORT_IN_DESC}
	</description>
	<propertyDefinitions>
		<propertyDefinition propertyName="host"
			label="${com.esri.geoevent.transport.awsiotmqtt-transport.TRANSPORT_IN_HOST_LBL}"
			description="${com.esri.geoevent.transport.awsiotmqtt-transport.TRANSPORT_IN_HOST_DESC}"
			propertyType="String" defaultValue="A1OAECNIGB9KXE.iot.us-west-2.amazonaws.com" mandatory="true"
			readOnly="false" />
		<propertyDefinition propertyName="port"
			label="${com.esri.geoevent.transport.awsiotmqtt-transport.TRANSPORT_IN_PORT_LBL}"
			description="${com.esri.geoevent.transport.awsiotmqtt-transport.TRANSPORT_IN_PORT_DESC}"
			propertyType="Integer" defaultValue="8883" mandatory="true" readOnly="false" />
			
	  	<propertyDefinition propertyName="rootCA"
			label="${com.esri.geoevent.transport.awsiotmqtt-transport.TRANSPORT_IN_ROOTCA_LBL}"
			description="${com.esri.geoevent.transport.awsiotmqtt-transport.TRANSPORT_IN_ROOTCA_DESC}"
			propertyType="String" defaultValue="C:/Users/venu4461/rootCA.pem" mandatory="true"
			readOnly="false" />		
			
		<propertyDefinition propertyName="certificate"
			label="${com.esri.geoevent.transport.awsiotmqtt-transport.TRANSPORT_IN_CERT_LBL}"
			description="${com.esri.geoevent.transport.awsiotmqtt-transport.TRANSPORT_IN_CERT_DESC}"
			propertyType="String" defaultValue="C:/Users/venu4461/cert.pem" mandatory="true"
			readOnly="false" />
			
		<propertyDefinition propertyName="privateKey"
			label="${com.esri.geoevent.transport.awsiotmqtt-transport.TRANSPORT_IN_PRIVATEKEY_LBL}"
			description="${com.esri.geoevent.transport.awsiotmqtt-transport.TRANSPORT_IN_PRIVATEKEY_DESC}"
			propertyType="String" defaultValue="C:/Users/venu4461/private.pem" mandatory="true"
			readOnly="false" />						
			
		<propertyDefinition propertyName="topic"
			label="${com.esri.geoevent.transport.awsiotmqtt-transport.TRANSPORT_IN_TOPIC_LBL}"
			description="${com.esri.geoevent.transport.awsiotmqtt-transport.TRANSPORT_IN_TOPIC_DESC}"
			propertyType="String" defaultValue="SBC/test" mandatory="true"
			readOnly="false" />
	</propertyDefinitions>
</transport>